{
  "name": "interview-assistant",
  "version": "1.0.0",
  "description": "AI-powered stealth interview assistance application",
  "main": "src/main-demo.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "demo": "electron src/main-demo.js",
    "build": "tsc",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "mocha --require ts-node/register tests/**/*.test.ts --timeout 10000",
    "test:e2e": "npm run build && mocha --require ts-node/register tests/e2e/setup.ts tests/e2e/workflow.test.ts --timeout 60000",
    "test:e2e:watch": "npm run test:e2e -- --watch"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/mocha": "^10.0.0",
    "@types/chai": "^4.3.0",
    "electron": "^27.0.0",
    "typescript": "^5.0.0",
    "mocha": "^10.0.0",
    "chai": "^4.3.0",
    "spectron": "^19.0.0",
    "ts-node": "^10.9.0"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "openai": "^4.0.0",
    "uuid": "^9.0.0"
  }
}